<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::system">
                    <resource id="Juashyam_LogViewer::system_log_report" title="Magento Logs and Reports" translate="title" sortOrder="80">
                        <resource id="Juashyam_LogViewer::system_log" title="Log" translate="title" sortOrder="10"/>
                        <resource id="Juashyam_LogViewer::system_report" title="Report" translate="title" sortOrder="20"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
